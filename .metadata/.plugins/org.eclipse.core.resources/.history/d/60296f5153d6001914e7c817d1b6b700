package homework2;
import java.util.Scanner;
public class AttackMonitor {
	public MonsterAttack[] reportAttacks() {
		MonsterAttack[] array = new MonsterAttack[5];
		for(int i = 0; i < array.length; i++) {
			System.out.println("Enter the name of the monster that caused attack " + (i + 1) + ": ");
			System.out.println("Enter the city that attack " + (i + 1) + "took place in: ");
			System.out.println("Enter the amount of damage in millions caused by attack " + (i + 1) + ": ");
			System.out.println("Enter the date of attack " + (i + 1) + "in the form: MM DD YYYY: ");
			
			
			
			
		}
	}
}
